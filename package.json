    "express": "^4.16.1",
    "express-jwt": "^5.3.1",
    "express-winston": "^2.4.0",
    "helmet": "^3.8.2",
    "isenv": "^0.1.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
