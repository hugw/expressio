  "main": "./dist",
  "scripts": {
    "build": "babel ./src --ignore '__test__/' -s -D -d dist",
    "demo-mongo": "nodemon -w ./demo-mongo -w ./src --inspect --exec \"babel-node ./demo-mongo\"",
    "demo-mongo:db": "babel-node ./demo-mongo/db/tasks",
    "demo-sequelize": "nodemon -w ./demo-sequelize -w ./src --inspect --exec \"babel-node ./demo-sequelize\"",
    "demo-sequelize:db": "babel-node ./demo-sequelize/db/tasks",
    "lint": "eslint .",
    "postinstall": "npm run build",
    "test": "jest . --verbose --colors --runInBand --forceExit",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
