{
  "name": "expressio",
  "version": "0.21.3",
  "description": "",
  "author": "Hugo W. <contact@hugw.io>",
  "main": "./lib/index.js",
  "license": "MIT",
  "repository": "https://github.com/hugw/expressio",
  "scripts": {
    "build": "babel ./src --ignore '__test__' -s -D -d lib",
    "clean": "rimraf lib",
    "dev": "nodemon -w ./src --inspect --exec \"babel-node ./src\"",
    "dev:jwt": "nodemon -w ./src --inspect --exec \"babel-node ./src/__test__/fixtures/jwt\"",
    "dev:mailer": "nodemon -w ./src --inspect --exec \"babel-node ./src/__test__/fixtures/mailer\"",
    "dev:core": "nodemon -w ./src --inspect --exec \"babel-node ./src/__test__/fixtures/core\"",
    "dev:database": "nodemon -w ./src --inspect --exec \"babel-node ./src/__test__/fixtures/database\"",
    "dev:database:tasks": "babel-node ./src/__test__/fixtures/database/db/tasks",
    "lint": "eslint ./src",
    "prepublishOnly": "npm run lint && npm run test && npm run clean && npm run build",
    "test": "jest ./src --verbose --colors --detectOpenHandles  --forceExit",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "helmet": "^3.12.1",
    "joi": "^13.4.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "ndtk": "^0.7.1",
    "nodemailer": "^4.6.7",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.38.0",
    "sqlite3": "^4.0.2",
    "umzug": "^2.1.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@hugw/eslint-config-node": "git+https://github.com/hugw/eslint-config-node.git",
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.13.0",
    "jest": "^23.3.0",
    "nodemon": "^1.17.5",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
