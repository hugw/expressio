    "eslint-plugin-import": "^2.17.3",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3",
    "supertest": "4.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  }
